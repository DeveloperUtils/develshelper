{% extends '::base.html.twig' %}

{% block title %}
  ProgrammingLanguage list
{% endblock %}

{% block content -%}
  <div class="box">
    <div class="box-header">
      <h3 class="box-title">ProgrammingLanguage list</h3>
      <div class="box-tools">
        <div class="input-group" style="width: 150px;">
          <input type="text" name="table_search" class="form-control input-sm pull-right" placeholder="Search">
          <div class="input-group-btn">
            <button class="btn btn-sm btn-default"><i class="fa fa-search"></i></button>
          </div>
        </div>
      </div>
    </div><!-- /.box-header -->

    <div class="box-body table-responsive no-padding">
      <table class="table table-hover records_list">
        <thead>
        <tr>
          <th>Name</th>
          <th>Canonical Name</th>
          <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
          <tr>
            <td><a class="label label-primary"
                   href="{{ path('wd_cs_admin_programminglanguage_show', { 'id': entity.id }) }}">{{ entity.name }}</a>
            </td>
            <td>{{ entity.nameCanonical }}</td>
            {#<td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>#}
            {#<td>{% if entity.updatedAt %}{{ entity.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>#}
            <td>
              <a href="{{ path('wd_cs_admin_programminglanguage_edit', { 'id': entity.id }) }}">
                <i class="fa fa-pencil-square-o"></i>
              </a>
              <a class=".btn.btn-app" href="{{ path('wd_cs_admin_programminglanguage_delete', { 'id': entity.id }) }}">
                <i class="fa fa-trash"></i></a>
            </td>
          </tr>
        {% endfor %}
        </tbody>
      </table>
    </div><!-- /.box-body -->
  </div><!-- /.box -->
  <div>
    <a class="btn btn-block btn-success" href="{{ path('wd_cs_admin_programminglanguage_new') }}">
      Create a new entry
    </a>
  </div>
{% endblock %}
