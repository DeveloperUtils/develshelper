{#{% form_theme form 'DhThemesCoreThemeBundle:form:fields.html.twig' %}#}

<!DOCTYPE html>
<html>
<head>
    {% block html_head %}
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <title>{% block title %}Welcome!{% endblock %}</title>

    <!-- Font Awesome -->
    {#<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">#}
    <!-- Ionicons -->
    {#<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">#}

    {% block head_scripts %}{% endblock %}

    {% stylesheets
        'bundles/dhthemescore3rdparty/bootstrap/css/bootstrap.min.css'
        'bundles/dhthemescore3rdparty/admin-lte/css/AdminLTE.min.css'
        'bundles/dhthemescore3rdparty/admin-lte/css/skins/_all-skins.min.css'
        'bundles/dhthemescore3rdparty/iCheck/flat/blue.css'
        'bundles/dhthemescore3rdparty/morris/morris.css'
        'bundles/dhthemescore3rdparty/jvectormap/jquery-jvectormap-1.2.2.css'
        'bundles/dhthemescore3rdparty/datepicker/datepicker3.css'
        'bundles/dhthemescore3rdparty/daterangepicker/daterangepicker-bs3.css'
        'bundles/dhthemescore3rdparty/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css'
        'bundles/dhthemescore3rdparty/css/ionicons.min.css'
        'bundles/dhthemescore3rdparty/css/font-awesome.min.css'
        'bundles/wdcsthemescoretheme/css/core-theme.css'
        filter='cssrewrite'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% block head_stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% endblock %}
</head>

{% block body %}
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        {% block body_header %}
            {% include 'DhThemesCoreThemeBundle:Default:blocks/header.top.html.twig' %}
        {% endblock %}


        <!-- Left side column. contains the logo and sidebar -->
        {% block body_sidebar %}
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                {% include 'DhThemesCoreThemeBundle:Default:blocks/nav-side-user.html.twig' %}
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
              <span class="input-group-btn">
                <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
              </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                {% include 'DhThemesCoreThemeBundle:Default:blocks/nav-side.html.twig' %}
            </section>
            <!-- /.sidebar -->
        </aside>
        {% endblock %}

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                {% block content_header %}
                <h1>
                    {{ block('title') }}
                    <small>{{ block('subtitle') }}</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li class="active">Dashboard</li>
                </ol>
                {% endblock content_header %}
            </section>

            <!-- Main content -->
            <section class="content">
                {% block content %}
                {% endblock %}
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
        {% block body_footer %}
        <footer class="main-footer">
            {% include 'DhThemesCoreThemeBundle:Default:blocks/footer.html.twig' %}
        </footer>
        {% endblock %}

        <!-- Control Sidebar -->
        {% block body_side_control %}
            {% include 'DhThemesCoreThemeBundle:Default:blocks/nav-right.html.twig' %}
        {% endblock %}
    </div><!-- ./wrapper -->

<!-- jQuery 2.1.4 -->
<script src="{{ asset('bundles/dhthemescore3rdparty/jQuery/jQuery-2.1.4.min.js') }}"></script>
<!-- jQuery UI 1.11.4 -->
<script src="{{ asset('bundles/dhthemescore3rdparty/jQueryUI/jquery-ui.min.js') }}"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
    $.widget.bridge('uibutton', $.ui.button);
</script>
<!-- Bootstrap 3.3.5 -->
<script src="{{ asset('bundles/dhthemescore3rdparty/bootstrap/js/bootstrap.min.js') }}"></script>
<!-- Morris.js charts -->
<script src="{{ asset('bundles/dhthemescore3rdparty/raphael/raphael-min.js') }}"></script>
<script src="{{ asset('bundles/dhthemescore3rdparty/morris/morris.min.js') }}"></script>
<!-- Sparkline -->
<script src="{{ asset('bundles/dhthemescore3rdparty/sparkline/jquery.sparkline.min.js') }}"></script>
<!-- jvectormap -->
<script src="{{ asset('bundles/dhthemescore3rdparty/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
<script src="{{ asset('bundles/dhthemescore3rdparty/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
<!-- jQuery Knob Chart -->
<script src="{{ asset('bundles/dhthemescore3rdparty/knob/jquery.knob.js') }}"></script>
<!-- daterangepicker -->
<script src="{{ asset('bundles/dhthemescore3rdparty/moment.js/moment.min.js') }}"></script>
<script src="{{ asset('bundles/dhthemescore3rdparty/daterangepicker/daterangepicker.js') }}"></script>
<!-- datepicker -->
<script src="{{ asset('bundles/dhthemescore3rdparty/datepicker/bootstrap-datepicker.js') }}"></script>
<!-- Bootstrap WYSIHTML5 -->
<script src="{{ asset('bundles/dhthemescore3rdparty/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
<!-- Slimscroll -->
<script src="{{ asset('bundles/dhthemescore3rdparty/slimScroll/jquery.slimscroll.min.js') }}"></script>
<!-- FastClick -->
<script src="{{ asset('bundles/dhthemescore3rdparty/fastclick/fastclick.min.js') }}"></script>
<!-- AdminLTE App -->
<script src="{{ asset('bundles/dhthemescore3rdparty/admin-lte/js/app.min.js') }}"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
{#<script src="{{ asset('bundles/dhthemescore3rdparty/admin-lte/js/pages/dashboard.js') }}"></script>#}
<!-- AdminLTE for demo purposes -->
{#<script src="{{ asset('bundles/dhthemescore3rdparty/admin-lte/js/demo.js') }}"></script>#}

    {% block javascripts %}{% endblock %}
</body>
{% endblock %}
</html>
